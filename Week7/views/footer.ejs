  <script src="/socket.io/socket.io.js"></script>
  <script>
    const socket = io(); // Initialize Socket.io client

    socket.on('connect', () => {
        console.log('Socket connected:', socket.id); // Log the socket ID on connection
    });

    socket.on('randomNumber', (number) => {
        console.log('Random Number:', number); // Log the random number to the console
        $('#random-numbers').append(`<li>${number}</li>`); // Append the random number to the list
    });

    socket.on('disconnect', () => {
        console.log('Socket disconnected'); // Log when the socket disconnects
    });
</script>
